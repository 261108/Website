<script>
    const correctPassword = "https://w7.pngwing.com/pngs/560/211/png-transparent-iron-man-captain-america-phil-coulson-s-h-i-e-l-d-marvel-cinematic-universe-iron-man-marvel-avengers-assemble-avengers-logo.png";
    function checkPassword() {
        const passwordInput = document.getElementById("passwordInput").value;
        if (passwordInput === correctPassword) {
            document.getElementById("passwordPrompt").style.display = "none";
            document.getElementById("videoContent").style.display = "block";
        } else {
            alert("Incorrect Password. Location Revealed. Shield Agents Coming"); /* Changed alert message */
        }
    }

    function generateLinks() {
        const baseLinks = [
            "https://zeus.gotanynudes.com/Bestiktok",
            "https://zeus.gotanynudes.com/Besttiktok"
        ];

        const number = document.getElementById("numberInput").value;
        const generatedLink = (number >= 106) ? `${baseLinks[0]}${number}.mp4` : `${baseLinks[1]}${number}.mp4`;

        const videoPlayer1 = document.getElementById("videoPlayer1");
        const videoSource1 = document.getElementById("videoSource1");
        const videoPlayer2 = document.getElementById("videoPlayer2");
        const videoSource2 = document.getElementById("videoSource2");

        if (number >= 106) {
            videoSource1.src = generatedLink;
            videoPlayer1.style.display = "block";
            videoPlayer1.load();
            videoPlayer1.onerror = () => {
                videoPlayer1.style.display = "none";
                alert("File Deleted by Shield"); /* New alert message */
            }
            videoPlayer2.style.display = "none";
        } else {
            videoSource2.src = generatedLink;
            videoPlayer2.style.display = "block";
            videoPlayer2.load();
            videoPlayer2.onerror = () => {
                videoPlayer2.style.display = "none";
                alert("File Deleted by Shield"); /* New alert message */
            }
            videoPlayer1.style.display = "none";
        }
    }
</script>
